#library "MEDSymbol"
#include "zcommon.acs"

script "SYMPreset" (void)
{
	SetCVar("custoggle", 1);
	
    If(GetCVar("pretoggle") == 0 && GetCVar("asmtoggle") == 0)
	{
		SetCVar("medtoggle", 0);
		SetCVar("stmtoggle", 0);
		SetCVar("brztoggle", 0);
	}
    Else If(GetCVar("pretoggle") == 1 && GetCVar("asmtoggle") == 0)
	{
		SetCVar("medtoggle", 1);
		SetCVar("stmtoggle", 1);
		SetCVar("brztoggle", 1);
	}
    Else If(GetCVar("pretoggle") == 2 && GetCVar("asmtoggle") == 0)
	{
		SetCVar("medtoggle", 2);
		SetCVar("stmtoggle", 2);
		SetCVar("brztoggle", 2);
	}
	Else If(GetCVar("asmtoggle") == 1)
	{
		SetCVar("custoggle", 0);
		SetResultValue(GetCVar("medtoggle"));
		SetResultValue(GetCVar("stmtoggle"));
		SetResultValue(GetCVar("brztoggle"));
	}
	
	Delay(1);
	Restart;
}

